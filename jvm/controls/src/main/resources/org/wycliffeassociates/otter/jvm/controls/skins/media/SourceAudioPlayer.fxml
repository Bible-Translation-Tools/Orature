<VBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" styleClass="audioplayer--scripture-takes" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <JFXSlider fx:id="audioSlider">
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin></JFXSlider>
      <HBox>
         <children>
            <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <Label styleClass="audioplayer--scripture-takes__label" text="Source Audio">
                     <graphic>
                        <FontIcon iconLiteral="gmi-hearing" iconSize="22" styleClass="audioplayer--scripture-takes__label__icon" />
                     </graphic>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <JFXButton fx:id="playBtn">
                     <graphic>
                        <FontIcon iconLiteral="mdi-play" iconSize="10" selectionEnd="1" styleClass="audioplayer--scripture-takes__play-btn__icon" />
                     </graphic>
                     <styleClass>
                        <String fx:value="audioplayer--scripture-takes__play-btn" />
                        <String fx:value="btn--cta" />
                     </styleClass>
                  </JFXButton>
               </children>
            </HBox>
            <HBox maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
